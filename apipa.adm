CLASS MACHINE
CATEGORY "Apipa"
KEYNAME "System\CurrentControlSet\Services\Tcpip\Parameters"
POLICY !!APIPAPolicy
EXPLAIN !!APIPAPolicy
VALUENAME "IPAutoconfigurationEnabled"
VALUEON NUMERIC 1
VALUEOFF NUMERIC 0
END POLICY
END CATEGORY
[strings]
APIPAPolicy="Automatic Private IP addressing"